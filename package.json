{
	"name": "meguro-nextjs",
	"version": "0.2.0",
	"description": "Portfolio site using Next.js",
	"author": "Alvaro Garcia",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"prebuild": "npm run clean:next & npm run test:types",
		"build": "next build",
		"prebuild:demo": "npm run clean:next & npm run test:types",
		"build:demo": "env USE_DEMO=1 next build",
		"clean:next": "rm -rf .next",
		"clean:out": "rm -rf out",
		"preexport": "npm run clean:out",
		"export": "npm run build && next export",
		"export:demo": "npm run build:demo && next export",
		"dev": "node server.js",
		"dev:demo": "env USE_DEMO=1 node server.js",
		"serve": "cross-env NODE_ENV=production node server.js",
		"test": "npm run test:types && npm run test:lint && npm run test:prettier && npm run test:unit",
		"test:lint": "eslint \"**/*.{js,ts,tsx}\"",
		"test:prettier": "prettier --check \"**/*.{js,ts,tsx,json}\"",
		"test:types": "tsc",
		"test:unit": "jest --watch",
		"prettier": "prettier --write \"**/*.{js,ts,tsx,json}\"",
		"deploy": "npm run export:demo && gh-pages -d out"
	},
	"dependencies": {
		"@mdx-js/loader": "^1.6.21",
		"@mdx-js/react": "^1.6.21",
		"@types/enzyme": "^3.10.8",
		"@types/jest": "^26.0.15",
		"@types/lodash.throttle": "^4.1.6",
		"@types/mdx-js__react": "^1.5.3",
		"@types/react": "^17.0.0",
		"@types/react-dom": "^17.0.0",
		"@zeit/next-stylus": "^1.0.1",
		"babel-loader": "^8.2.1",
		"cross-env": "^7.0.2",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.5",
		"express": "^4.17.1",
		"file-loader": "^6.2.0",
		"glob": "^7.1.6",
		"gray-matter": "^4.0.2",
		"jest": "^26.6.3",
		"jest-environment-enzyme": "^7.1.2",
		"jest-enzyme": "^7.1.2",
		"lodash.throttle": "^4.1.1",
		"next": "^10.0.3",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-scrollbars-custom": "^4.0.25",
		"remark-unwrap-images": "^2.0.0",
		"responsive-loader": "^2.2.0",
		"sharp": "^0.26.3",
		"stylus": "^0.54.8",
		"typescript": "^4.1.2"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^4.8.2",
		"@typescript-eslint/parser": "^4.8.2",
		"eslint": "^7.14.0",
		"eslint-config-prettier": "^6.15.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.21.5",
		"gh-pages": "^3.1.0",
		"prettier": "^2.2.0"
	}
}
