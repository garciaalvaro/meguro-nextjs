{
	"name": "meguro-nextjs",
	"version": "0.1.0",
	"description": "Portfolio site using Next.js",
	"author": "Alvaro Garcia",
	"license": "MIT",
	"private": true,
	"scripts": {
		"start": "npm run dev",
		"build": "next build",
		"dev": "node server.js",
		"serve": "cross-env NODE_ENV=production node server.js",
		"test": "npm run test:type && npm run test:lint && npm run test:prettier && npm run test:unit",
		"test:lint": "eslint \"**/*.{js,ts,tsx}\"",
		"test:prettier": "prettier --check \"**/*.{js,json,ts,tsx}\"",
		"test:types": "tsc",
		"test:unit": "jest --watch",
		"prettier": "prettier --write \"**/*.{js,json,ts,tsx}\""
	},
	"dependencies": {
		"lodash.throttle": "^4.1.1",
		"next": "^9.5.3",
		"react": "^16.13.1",
		"react-dom": "^16.13.1"
	},
	"devDependencies": {
		"@mdx-js/loader": "^1.6.18",
		"@mdx-js/react": "^1.6.18",
		"@types/enzyme": "^3.10.6",
		"@types/jest": "^26.0.14",
		"@types/lodash.throttle": "^4.1.6",
		"@types/mdx-js__react": "^1.5.2",
		"@types/react": "^16.9.49",
		"@types/react-dom": "^16.9.8",
		"@typescript-eslint/eslint-plugin": "^4.1.1",
		"@typescript-eslint/parser": "^4.1.1",
		"@zeit/next-stylus": "^1.0.1",
		"babel-loader": "^8.1.0",
		"cross-env": "^7.0.2",
		"enzyme": "^3.11.0",
		"enzyme-adapter-react-16": "^1.15.4",
		"eslint": "^7.9.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"eslint-plugin-react": "^7.20.6",
		"express": "^4.17.1",
		"gray-matter": "^4.0.2",
		"jest": "^26.4.2",
		"jest-environment-enzyme": "^7.1.2",
		"jest-enzyme": "^7.1.2",
		"prettier": "^2.1.2",
		"remark-unwrap-images": "^2.0.0",
		"stylus": "^0.54.8",
		"typescript": "^4.0.3"
	}
}
